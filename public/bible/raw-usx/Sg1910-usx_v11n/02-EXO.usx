      <verse eid="EXO 1:14" />
      <verse number="15" style="v" sid="EXO 1:15" />
      Le <char style="w" strong="H4428">roi</char> d’<char style="w" strong="H4714">Égypte</char> <char style="w" strong="H0559">parla</char> aussi aux <char style="w" strong="H3205">sages-femmes</char> des <char style="w" strong="H5680">Hébreux</char><char style="w" strong="H0834">,</char> <char style="w" strong="H8034">nommées</char> l’<char style="w" strong="H0259">une</char> <char style="w" strong="H8236">Schiphra</char>, <char style="w" strong="H8034">et</char> l’<char style="w" strong="H8145">autre</char> <char style="w" strong="H6326">Pua</char>.
      <verse eid="EXO 1:15" />
